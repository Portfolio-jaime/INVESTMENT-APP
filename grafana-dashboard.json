{
  "dashboard": {
    "id": null,
    "title": "Analysis Engine Metrics",
    "tags": ["analysis-engine", "prometheus"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "refId": "A",
            "legendFormat": "{{method}} {{path}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "yaxes": [
          {
            "format": "reqps",
            "label": null
          },
          {
            "format": "short",
            "label": null
          }
        ]
      },
      {
        "id": 2,
        "title": "HTTP Request Duration (95th percentile)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "95th percentile"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "yaxes": [
          {
            "format": "s",
            "label": null
          },
          {
            "format": "short",
            "label": null
          }
        ]
      },
      {
        "id": 3,
        "title": "HTTP Status Codes",
        "type": "table",
        "targets": [
          {
            "expr": "sum(increase(http_requests_total[5m])) by (status)",
            "refId": "A",
            "instant": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "left"
            }
          },
          "overrides": []
        }
      },
      {
        "id": 4,
        "title": "Total HTTP Requests",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(http_requests_total[5m]))",
            "refId": "A",
            "instant": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            }
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}